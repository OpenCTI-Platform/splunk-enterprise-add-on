  "dataSources": {
    "ds_N5ccv7a7_ds_Pm9AHvFu_ds_hsDs3Q0C": {
      "name": "OpenCTI IOCs per indicator_type copy 1",
      "options": {
        "query": "| inputlookup opencti_indicators | stats count by indicator_types"
      },
      "type": "ds.search"
    },
    "ds_PA2GTclu_ds_hsDs3Q0C": {
      "name": "OpenCTI IOCs per markings",
      "options": {
        "query": "| inputlookup opencti_indicators | stats count by markings",
        "queryParameters": {
          "earliest": "$global_time.earliest$",
          "latest": "$global_time.latest$"
        }
      },
      "type": "ds.search"
    },
    "ds_PYxsaacd": {
      "name": "OpenCTI Indicators Count",
      "options": {
        "query": "| inputlookup opencti_indicators | stats count"
      },
      "type": "ds.search"
    },
    "ds_Pm9AHvFu_ds_hsDs3Q0C": {
      "name": "OpenCTI IOCs per indicator_type",
      "options": {
        "query": "| inputlookup opencti_indicators | stats count by indicator_types",
        "queryParameters": {
          "earliest": "$global_time.earliest$",
          "latest": "$global_time.latest$"
        }
      },
      "type": "ds.search"
    },
    "ds_R0rMZ4Ao": {
      "name": "OpenCTI Indicators per Labels",
      "options": {
        "query": "| inputlookup opencti_indicators | stats count by labels",
        "queryParameters": {
          "earliest": "$global_time.earliest$",
          "latest": "$global_time.latest$"
        }
      },
      "type": "ds.search"
    },
    "ds_YI12Xgn7": {
      "name": "OpenCTI added IOCs over time",
      "options": {
        "query": "| inputlookup opencti_indicators | eval added_at = strptime(added_at, \"%Y-%m-%dT%H:%M:%SZ\") | eval _time = added_at | timechart count AS Count",
        "queryParameters": {
          "earliest": "$global_time.earliest$",
          "latest": "$global_time.latest$"
        }
      },
      "type": "ds.search"
    },
    "ds_hsDs3Q0C": {
      "name": "OpenCTI IOCs per type",
      "options": {
        "query": "| inputlookup opencti_indicators | stats count by type",
        "queryParameters": {
          "earliest": "$global_time.earliest$",
          "latest": "$global_time.latest$"
        }
      },
      "type": "ds.search"
    },
    "ds_nbAJEARh_ds_Pm9AHvFu_ds_hsDs3Q0C": {
      "name": "OpenCTI IOCs per score",
      "options": {
        "query": "| inputlookup opencti_indicators | stats count by score",
        "queryParameters": {
          "earliest": "$global_time.earliest$",
          "latest": "$global_time.latest$"
        }
      },
      "type": "ds.search"
    },
    "ds_tR29ZhaJ_ds_PA2GTclu_ds_hsDs3Q0C": {
      "name": "OpenCTI IOCs per Creator",
      "options": {
        "query": "| inputlookup opencti_indicators | stats count by created_by",
        "queryParameters": {
          "earliest": "$global_time.earliest$",
          "latest": "$global_time.latest$"
        }
      },
      "type": "ds.search"
    },
    "ds_z5ZhhLJx_ds_PYxsaacd": {
